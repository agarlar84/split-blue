{
    "name": "bernardosecades/split-blue",
    "description": "Split Blueprint files (request/response sections) to generate documentation and mock servers files",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Bernardo Secades",
            "email": "bernardosecades@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "BernardoSecades\\SplitBlue\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^5.5|^7.0",
        "symfony/console": "^2.2|^3.0",
        "symfony/process": "^2.2|^3.0",
        "symfony/finder": "^2.2|^3.0",
        "symfony/filesystem": "^2.2|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3",
        "bernardosecades/packagist-security-checker": "dev-master"
    },
    "suggest": {
        "https://github.com/jreese/markdown-pp": "The Markdown Preprocessor is a Python module designed to add extended features on top of the excellent Markdown syntax defined by John Grub",
        "https://github.com/danielgtaylor/aglio": "An API Blueprint renderer"
    }
}
